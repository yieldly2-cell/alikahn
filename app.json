{
  "expo": {
    "name": "Yieldly",
    "slug": "yieldly",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "scheme": "yieldly",
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.yieldly.app"
    },
    "android": {
      "package": "com.yieldly.app",
      "adaptiveIcon": {
        "backgroundColor": "#0A0A0A"
      }
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
